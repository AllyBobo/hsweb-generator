{
  "meta.header": {
    "name": {
      "title": "名称",
      "default": ""
    },
    "javaType": {
      "title": "java类",
      "type": "select",
      "option_list": [
        "byte",
        "short",
        "char",
        "boolean",
        "int",
        "double",
        "float",
        "string",
        "date"
      ],
      "default": "String",
      "mapperBind": "dataType"
    },
    "dataType": {
      "title": "数据类型",
      "default": "varchar",
      "lowerCase": true,
      "mapper": {
        "^varchar.*": "string",
        "number": "double",
        "number\\(([1-9]\\d{0,2}+)\\)": "int",
        "number\\(([1-9]\\d{0,2}+,[1-9]\\d{0,2}+)\\)": "double",
        "clob": "string",
        "date": "date",
        "timestamp\\(([1-9]\\d{0,2}+)\\)",
        "timestamp": "date"
      }
    },
    "comment": {
      "title": "备注",
      "default": "新建字段"
    },
    "defaultValue": {
      "title": "默认值",
      "default": ""
    },
    "primaryKey": {
      "title": "是否主键",
      "type": "boolean",
      "default": "false"
    },
    "notNull": {
      "title": "不能为空",
      "type": "boolean",
      "default": "false"
    }
  }
}